{
  "expo": {
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#ffffff"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/logo.png",
      "color": "#000"
    },
    "name": "FlipPaper",
    "slug": "FlipPaper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ventus69.flippaper",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.511105291379-67vrreu8ta795ddb22idmkdirtukfhb6"
        }
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["NOTIFICATIONS"],
      "package": "com.ventus69.flippaper"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
